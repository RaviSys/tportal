<div class="container">
  <div class="row">
    <div class="col-md-7 col-12">
      <div class="row">
        <div class="col-md-6 mb-3">
          <%= link_to assign_tutors_admin_tutor_programs_path do %>
            <div class="card-body shadow rounded-0 border-cc">
              <div class="text-center">
                <%= image_tag "user-icon.png", class: "img-custom" %>
                <h5 class="text-primary">Assign programs to Tutors</h5>
              </div>
            </div>
          <% end %>
        </div>
        <div class="col-md-6 mb-3">
          <%= link_to assign_participants_admin_participant_programs_path do %>
            <div class="card-body shadow rounded-0 border-cc">
              <div class="text-center">
                <%= image_tag "user-icon.png", class: "img-custom" %>
                <h5 class="text-primary">Assign programs to Participants</h5>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
